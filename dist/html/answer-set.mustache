<div id="{{id}}" class="answer-set" data-ali="answer-set">
	{{#questions}}
		<form data-ali-correct="1" data-ali-correct-message="{{correct}}" data-ali-incorrect-message="{{incorrect}}">
			<label for="q-1">{{stem}}</label>
			<div class="answer-set-select">
				<select id="q-1">
					<option>Select...</option>
					{{#options}}
						<option>{{name}}</option>
					{{/options}}
				</select>
			</div>
			<div class="answer-set-submit">
				<button type="submit">Submit</button>
			</div>
		</form>
	{{/questions}}
	{{#incorrectFeedback}}
		<div class="feedback-content incorrect">
			{{{incorrectFeedback}}}
		</div>
	{{/incorrectFeedback}}
	{{#correctFeedback}}
		<div class="feedback-content incorrect">
			{{{correctFeedback}}}
		</div>
	{{/correctFeedback}}

	{{#incorrectDialog}}
		<div class="feedback-content incorrect">
			{{{incorrectDialog}}}
		</div>
	{{/incorrectDialog}}
	{{#correctDialog}}
		<div class="feedback-content incorrect">
			{{{correctDialog}}}
		</div>
	{{/correctDialog}}
</div>