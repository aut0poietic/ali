<form id="{{id}}" class="ordered-items" data-ali="ordered-items"{{#incorrectNotice}} data-ali-notice-incorrect="{{incorrectNotice}}" {{/incorrectNotice}}{{#correctNotice}} data-ali-notice-correct="{{correctNotice}}"{{/correctNotice}}>
	<p>{{{stem}}}</p>
	<ul class="list-elements">
		<li data-correct="{{correctIndex}}"><label>{{{label}}}</label></li>
	</ul>
	<div class="submit-row">
		<button class="submit" aria-label="Submit">Submit</button>
	</div>
	{{#incorrectFeedback}}
		<div class="feedback-content incorrect">
			{{{incorrectFeedback}}}
		</div>
	{{/incorrectFeedback}}
	{{#correctFeedback}}
		<div class="feedback-content incorrect">
			{{{correctFeedback}}}
		</div>
	{{/correctFeedback}}

	{{#incorrectDialog}}
		<div class="feedback-content incorrect">
			{{{incorrectDialog}}}
		</div>
	{{/incorrectDialog}}
	{{#correctDialog}}
		<div class="feedback-content incorrect">
			{{{correctDialog}}}
		</div>
	{{/correctDialog}}
</form>
